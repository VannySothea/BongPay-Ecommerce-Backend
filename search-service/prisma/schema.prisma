generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SearchProduct {
  id Int @id @default(autoincrement())
  productId Int @unique()
  name String
  shortDesc String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}